<?xml version="1.0"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark"
		 xmlns:fills="ssen.components.fills.*" xmlns:graphics="ssen.components.graphics.*">
	<s:Rect width="100%" height="100%">
		<s:fill>
			<fills:Stripe angle="135">
				<fills:StripeEntry color="#000000" alpha="0.3" size="10" />
				<fills:StripeEntry color="#000000" alpha="0.1" size="15" />
			</fills:Stripe>
		</s:fill>
	</s:Rect>
	<graphics:LineBox width="100%" height="100%"
					  lineWeightTop="30"
					  lineWeightBottom="30"
					  lineWeightLeft="30"
					  lineWeightRight="30">
		<graphics:fill>
			<fills:Stripe angle="135">
				<fills:StripeEntry color="#000000" alpha="0.5" size="10" />
				<fills:StripeEntry color="#000000" alpha="0.3" size="15" />
			</fills:Stripe>
		</graphics:fill>
	</graphics:LineBox>
</s:Group>
